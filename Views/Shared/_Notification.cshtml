<!-- Display Message to the next page-->
@if (TempData["success"] != null)
{

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script type="text/javascript">
        Swal.fire(
            '@TempData["success"]',
            '',
            'success'
        )
    </script>
}

@if (TempData["error"] != null)
{
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script type="text/javascript">
        Swal.fire(
            '@TempData["success"],
            '',
            'error'
        )
    </script>

}